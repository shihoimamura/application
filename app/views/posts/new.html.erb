<%= form_with(model: Post.new, local: true) do |form| %>
    <div class="post_picture">
      <%= form.label :picture %>
      <%= form.text_field :picture %>
    </div>
    <div class="post_content">
      <%= form.label :content %>
      <%= form.text_field :content %>
    </div>
    <%= form.submit %>
  <% end %>